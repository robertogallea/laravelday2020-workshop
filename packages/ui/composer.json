{
    "name": "workshop/ui",
    "type": "library",
    "description": "Ui for project",
    "require": {
        "orchestra/testbench": "*",
        "workshop/core": "*"
    },
    "autoload": {
        "psr-4": {
            "Workshop\\Ui\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workshop\\Ui\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        }
    ]
}
